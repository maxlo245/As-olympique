{
    "name": "maxlo245/as-olympique",
    "description": "AS Olympique Saint-RÃ©my - OWASP Cybersecurity Training Application",
    "type": "project",
    "keywords": ["owasp", "security", "education", "training", "cybersecurity"],
    "license": "MIT",
    "authors": [
        {
            "name": "maxlo245",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-fileinfo": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "AsOlympique\\": "src/classes/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AsOlympique\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
