{
    "name": "as-olympique/cybersecurity-training",
    "description": "AS Olympique Saint-RÃ©my - Cybersecurity Training Application",
    "type": "project",
    "keywords": ["owasp", "security", "training", "education", "php", "cybersecurity"],
    "license": "MIT",
    "authors": [
        {
            "name": "Florence PEYRATAUD",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "AsOlympique\\Core\\": "src/Core/",
            "AsOlympique\\Secure\\": "src/Secure/",
            "AsOlympique\\Vulnerable\\": "src/Vulnerable/",
            "AsOlympique\\Utils\\": "src/Utils/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AsOlympique\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs --standard=PSR12 src/",
        "cs-fix": "phpcbf --standard=PSR12 src/"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
